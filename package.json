    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "body-parser": "^2.2.0",
    "canvas": "^2.11.2",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.0.9",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
